{
  "name": "bundlephobia-inline",
  "version": "0.1.1",
  "description": "A lightweight wrapper around react-query and the bundlephobia API",
  "license": "MIT",
  "homepage": "https://github.com/NateRadebaugh/bundlephobia-inline",
  "repository": {
    "type": "git",
    "url": "https://github.com/NateRadebaugh/bundlephobia-inline"
  },
  "author": "Nate Radebaugh",
  "main": "dist/index.js",
  "umd:main": "dist/bundlephobia-inline.umd.production.min.js",
  "module": "dist/bundlephobia-inline.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "react-query",
    "bundlephobia",
    "hooks"
  ],
  "scripts": {
    "now-build": "build-storybook -c .storybook -o public",
    "build": "tsdx build --format=umd,esm,cjs",
    "dev": "tsdx watch --format=esm",
    "start": "yarn dev",
    "prettier": "prettier --write src/*.{json,js,jsx,ts,tsx,scss} *.{json,js,md,mdx}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "chromatic": "chromatic"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "babel-loader": "^8.1.0",
    "chromatic": "^4.0.2",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "ts-loader": "^7.0.3",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "react-query": "^1.3.4"
  }
}
